  <select id="getProductsByUsername" resultMap="BaseResultMap">
   SELECT * FROM Product p INNER JOIN ShoppingCart sc ON p.prodid = sc.prodid WHERE sc.username = #{username}
  </select>

  <select id="getProductsToWishList" resultMap="BaseResultMap">
   SELECT * FROM Product p INNER JOIN WishList wl ON p.prodid = wl.prodid WHERE wl.username = #{username}
  </select>

